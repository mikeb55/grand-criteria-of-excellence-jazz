    <!-- VARIATION 2: Rhythmic Displacement -->
    <measure number="5">
      <direction placement="above">
        <direction-type><words font-size="14" font-weight="bold">VARIATION 2: Rhythmic</words></direction-type>
      </direction>
      <direction placement="above">
        <direction-type><words font-size="10" font-style="italic">Displaced accents, syncopation</words></direction-type>
      </direction>
      <harmony><root><root-step>A</root-step><root-alter>-1</root-alter></root><kind>major-ninth</kind></harmony>
      <!-- Eb displaced to beat 2 -->
      <note><rest/><duration>24</duration><type>quarter</type></note>
      <note><pitch><step>E</step><alter>-1</alter><octave>5</octave></pitch><duration>36</duration><type>quarter</type><dot/><notations><accent/></notations></note>
