      <direction placement="above">
        <direction-type><words font-size="14" font-weight="bold">VARIATION 1: Melodic</words></direction-type>
      </direction>
      <direction placement="above">
        <direction-type><words font-size="10" font-style="italic">Embellished melody with ornaments</words></direction-type>
        <sound tempo="60"/>
      </direction>
      <harmony><root><root-step>A</root-step><root-alter>-1</root-alter></root><kind>major-ninth</kind></harmony>
      <!-- Original: Eb whole. Variation: approach notes + Eb -->
      <note><pitch><step>D</step><alter>-1</alter><octave>5</octave></pitch><duration>12</duration><type>eighth</type></note>
      <note><pitch><step>D</step><octave>5</octave></pitch><duration>12</duration><type>eighth</type></note>
      <note><pitch><step>E</step><alter>-1</alter><octave>5</octave></pitch><duration>48</duration><type>half</type></note>
