    </measure>
    <measure number="7">
      <harmony><root><root-step>G</root-step><root-alter>-1</root-alter></root><kind>major-seventh</kind></harmony>
      <!-- F with anticipation -->
      <note><pitch><step>F</step><octave>5</octave></pitch><duration>12</duration><type>eighth</type></note>
      <note><rest/><duration>12</duration><type>eighth</type></note>
      <note><pitch><step>F</step><octave>5</octave></pitch><duration>36</duration><type>quarter</type><dot/><notations><accent/></notations></note>
      <note><rest/><duration>12</duration><type>eighth</type></note>
      <note><pitch><step>F</step><octave>5</octave></pitch><duration>24</duration><type>quarter</type></note>
    </measure>
    <measure number="8">
      <harmony><root><root-step>C</root-step></root><kind>minor-seventh</kind></harmony>
      <note><rest/><duration>24</duration><type>quarter</type></note>
      <note><pitch><step>B</step><alter>-1</alter><octave>4</octave></pitch><duration>24</duration><type>quarter</type></note>
      <harmony><root><root-step>F</root-step></root><kind>minor-seventh</kind></harmony>
      <note><rest/><duration>24</duration><type>quarter</type></note>
      <note><pitch><step>A</step><alter>-1</alter><octave>4</octave></pitch><duration>24</duration><type>quarter</type></note>
